  padding: 11px 9px 11px 4px;
  display: inline-block;
  border-radius: 0 40px 40px 0;
  float: left;
  height: 40px;
  margin-top: 31px;
}
.data-heads{
  list-style: none;
}
.data-head-list{
  border-bottom: 1px solid #ddd;
  font-size: 18px;
  counter-increment: step-counter;
  margin-left: -20px;
  position: relative;
  height: 100px;
}
.data-head-list .list-span > a{    
  color:#fff;  
  cursor: pointer;    
}

.data-head-list > a{
  color: #fff;
}

.list-span{
  top: 50%;
  display: block;
  margin-left: 45px;
  position: absolute;
  transform: translateY(-50%);
}
.user-heading{
  font-size: 26px;
  text-align: center;
  border-bottom: 1px solid #31859c; 
  color: #31859c; 
  margin-bottom: 20px;
  padding-bottom: 7px;
}
.page-heading{
  font-size: 26px;
  text-align: center;
  border-bottom: 1px solid #31859c; 
  color: #31859c;
  margin-bottom: 19px;
  padding-bottom: 7px;
}


